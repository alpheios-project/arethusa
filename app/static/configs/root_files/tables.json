{
  "main" : {
    "debug" : true,
    "showKeys" : true,
    "@include" : "static/configs/layouts/tables.json",
    "chunkParam" : "chunk",
    "auxConfPath": "http://services.perseids.org/arethusa-configs",
    "retrievers" : {
      "TreebankRetriever" : {
        "resource" : "arethusaServerTreebank",
        "docIdentifier" : "treebank",
        "preselector" : "w"
      }
    },
    "persisters" : {
      "TreebankPersister" : {
        "resource" : "arethusaServerTreebank",
        "docIdentifier" : "treebank"
      }
    },
    "plugins" : [
      "table"
    ]
  },

  "navbar" : {
    "template" : "js/templates/navbar1.html",
    "search" : true,
    "navigation" : true
  },

  "notifier" : {
    "duration" : "5000",
    "maxMessages" : 7
  },

  "navigator" : {
    "chunkSize" : 1
  },

  "resources" : {
    "@include" : "static/configs/resources/dev.json"
  },

  "plugins" : {
    "table" : { "@include":"js/arethusa.table/configs/conf.json" }
  },

  "exitHandler" : {
    "title" : "LLT",
    "route" : "http://latin-languge-toolkit.net"
  },

  "keyCapture" : {
    "@include" : "static/configs/keyboard/key_map.json"
  }
}